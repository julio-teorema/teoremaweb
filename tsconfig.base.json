{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2022",
    "useDefineForClassFields": false,
    "module": "esnext",
    "moduleResolution": "bundler",
    "types": [
      "node"
    ],
    "lib": [
      "es2022",
      "dom"
    ],
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "paths": {
      "@org/shared/models": [
        "packages/shared/models/src/index.ts"
      ],
      "@org/shared/mocks": [
        "packages/shared/mocks/src/index.ts"
      ],
      "@org/shared/services": [
        "packages/shared/services/src/index.ts"
      ],
      "@org/shared/ui-components": [
        "packages/shared/ui-components/src/index.ts"
      ],
      "@org/core/auth": [
        "packages/core/auth/src/index.ts"
      ]
    }
  },
  "files": [],
  "references": []
}